{
    "name": "digivia/form-handler",
    "type": "symfony-bundle",
    "description": "Provides form handler for Symfony form types",
    "keywords": ["symfony", "form", "handler", "factory", "form handler", "handling forms"],
    "homepage": "https://github.com/Digivia/form-handler",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric BATARSON",
            "email": "eric.batarson@digivia.fr"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^4.0|^5.0",
        "symfony/dependency-injection": "^4.0|^5.0",
        "symfony/http-kernel": "^4.0|^5.0",
        "symfony/form": "^4.0|^5.0",
        "symfony/http-foundation": "^4.0|^5.0"
    },
    "suggest": {
        "ext-intl": "*"
    },
    "autoload": {
        "psr-4": { "Digivia\\FormHandler\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Digivia\\FormHandler\\Tests\\": "tests/"
        }
    },
    "target-dir": "digivia/form-handler",
    "require-dev": {
        "symfony/phpunit-bridge": "^4.0|^5.0"
    }
}
