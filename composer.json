{
    "name": "digivia/form-handler",
    "type": "symfony-bundle",
    "description": "Provides form handler for Symfony form types",
    "keywords": ["symfony", "form", "handler", "factory", "form handler", "handling forms"],
    "homepage": "https://github.com/Digivia/form-handler",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric BATARSON",
            "email": "eric.batarson@digivia.fr"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "symfony/form": "^5.4 || ^6.0",
        "symfony/http-foundation": "^5.4 || ^6.0",
        "symfony/config": "^5.4 || ^6.0"
    },
    "suggest": {
        "ext-intl": "*"
    },
    "autoload": {
        "psr-4": { "Digivia\\FormHandler\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Digivia\\FormHandler\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/validator": "^5.4 || ^6.0"
    }
}
